    "test:watch": "jest --coverage --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@types/jest": "^27.0.2",
    "eslint": "^8.2.0",
    "jest": "^27.3.1"
  }
}
